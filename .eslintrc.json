{
    "env": {
        "es2021": true,
        "node": true,
        "browser": true
    },
    "extends": [
        "chase"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-continue": "off",
        "import/no-cycle": [
            "error",
            {
                "maxDepth": 1
            }
        ]
    },
    "globals": {}
}
